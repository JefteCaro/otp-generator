{
    "name": "jefte/otpgenerator",
    "description": "Laravel plugin to generate OTP Tokens",
    "keywords": [
        "otp", "token", "generator"
    ],
    "type": "library",
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7.0|^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jefte\\OTPGenerator\\": "src"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Jefte Caro",
            "email": "carojefte@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Jefte\\OTPGenerator\\OTPServiceProvider"
            ],
            "aliases": {
                "OTPGenerator": "Jefte\\OTPGenerator\\OTPGenerator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
